
export PATH=$HOME/bin:$HOME/.local/share/bin:/usr/local/bin:$PATH



export PATH=$HOME/basescripts/utilities/binaries:$PATH

# . /home/$USER/.nix-profile/etc/profile.d/nix.sh#



if [ -f ~/.helpers/aliases.txt ]; then
    # Echo the command to add it to .zshrc
    source ~/.helpers/aliases.txt
  
fi


alias vi="nvim"
alias vim="nvim" 

alias venvcmd='eval "$(cat ~/.helpers/venvcmd.txt)"'

export EDITOR=nvim
POWERLEVEL9K_DISABLE_CONFIGURATION_WIZARD=true
export BROWSER='chrome'
export CHROME_EXECUTABLE="/usr/bin/google-chrome-stable"




# To customize prompt, run `p10k configure` or edit ~/.p10k.zsh.

export cfg="$HOME/.config"



alias sstart='sudo systemctl start'
alias srestart='sudo systemctl restart'
alias sstop='sudo systemctl stop'
alias sdisable='sudo systemctl disable'
alias sstatus='sudo systemctl status'
alias spu="sudo pacman -S archlinux-keyring --noconfirm && sudo pacman -Syu"
